<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <PropertyGroup>
    <PackageId>A2A-Protocol</PackageId>
    <Version>0.1.0</Version>
    <Authors>Mentessa</Authors>
    <Company>Mentessa</Company>
    <Description>Open protocol for agent-to-agent communication</Description>
    <PackageProjectUrl>https://github.com/mentessaas/a2a-protocol</PackageProjectUrl>
    <RepositoryUrl>https://github.com/mentessaas/a2a-protocol</RepositoryUrl>
    <License>MIT</License>
  </PropertyGroup>

</Project>
